    "scripts": {
        "generate-sitemap:dev": "env-cmd -f ../server/src/envs/.env node ../server/src/api/middlewares/sitemapMiddleware.js",
        "generate-sitemap:prod": "env-cmd -f ../server/src/envs/.env.prod node ../server/src/api/middlewares/sitemapMiddleware.js",
        "build": "rm -rf build && (yarn generate-sitemap:prod) && webpack --config './webpack/webpack.config.js'",
        "build:dev": "rm -rf build && (yarn generate-sitemap:dev) && webpack --config './webpack/webpack.config.js'",
        "build:demo": "rm -rf demo-build && webpack --config './webpack/webpack.demo.config.js'",
        "build:staging": "rm -rf staging-build && webpack --config './webpack/webpack.staging.config.js'",
        "build:qa": "rm -rf qa-build && webpack --config './webpack/webpack.qa.config.js'",
        "build:uat": "rm -rf uat-build && webpack --config './webpack/webpack.uat.config.js'",
        "build:prod": "rm -rf prod-build && webpack --config './webpack/webpack.prod.config.js'",
        "lint:css": "stylelint './src/**/*.tsx'",
        "lint:js": "eslint . --ext .ts,.tsx,.js --ignore-pattern 'node_modules/' --ignore-pattern 'prod-build/'",
        "test": "jest --verbose",
        "percy": "percy exec -- cypress run",
        "serve": "webpack serve --config ./webpack/webpack.client-dev.config.js"
    },
